#include<stdio.h>
main() {
       int n=4;
       int i,j,k,l row;
       printf("enter the number of row: ");
       scanf("%d" ,&row);
       i=1;
       do {
           j=1;
           do {
              printf("%d");
              j++;
           } while (j<=i);
           printf("\n");
           i++;
       }while (i<= row);
       return 0;
}//end main    
